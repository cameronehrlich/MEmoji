OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers" -isystem "${PODS_ROOT}/Headers/GPUImage" -isystem "${PODS_ROOT}/Headers/MBProgressHUD" -isystem "${PODS_ROOT}/Headers/MagicalRecord" -isystem "${PODS_ROOT}/Headers/UIColor+Hex" -isystem "${PODS_ROOT}/Headers/UIImage+Additions"
OTHER_LDFLAGS = -ObjC -framework AVFoundation -framework CoreData -framework CoreGraphics -framework CoreMedia -framework OpenGLES -framework QuartzCore -framework UIKit
PODS_ROOT = ${SRCROOT}/Pods